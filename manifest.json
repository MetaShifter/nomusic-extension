{
  "manifest_version": 3,
  "name": "nomusic",
  "description": "no music.youtube.com",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://music.youtube.com/*"],
      "js": ["scripts/main.js"],
      "css": ["style/main.css"]
    }
  ]
}
